{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/log-{Date}.log",
          "retainedFileCountLimit": 5
        }
      },
      {
        "Name": "Trace"
      }
    ]
  },
  "App": {
    "Jwt": {
      "Issuer": "my app",
      "Audience": "client app",
      "SecretKey": "this is my secret key it is totally secure"
    }
  }
}
